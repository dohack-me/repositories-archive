from Crypto.Util.number import *

flag = b"blahaj{3x73nd3D_an0Mal0u5_cURV3_a77ACK_0n_F13LD_0F_P-aD1c_nUMb3r5}"
m = bytes_to_long(flag)

p = 62378614932334187184414530320857436260024498342816529064327089839152687534143
z = ceil(log(m, p))+1 # make sure that p^z > m

Zpz = Zmod(p^z)
EZpz = EllipticCurve(Zpz, (314159, 271828))

G = EZpz.lift_x(2147483647)

print((m*G).xy())

